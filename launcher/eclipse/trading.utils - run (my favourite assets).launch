<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<launchConfiguration type="org.eclipse.jdt.launching.localJavaApplication">
    <listAttribute key="org.eclipse.debug.core.MAPPED_RESOURCE_PATHS">
        <listEntry value="/trading-utils/src/main/java/org/rg/service/Application.java"/>
    </listAttribute>
    <listAttribute key="org.eclipse.debug.core.MAPPED_RESOURCE_TYPES">
        <listEntry value="1"/>
    </listAttribute>
    <mapAttribute key="org.eclipse.debug.core.environmentVariables">
        <mapEntry key="binanceApiKey" value="${env_var:BINANCE_API_KEY}"/>
        <mapEntry key="binanceApiSecret" value="${env_var:BINANCE_API_SECRET}"/>
        <mapEntry key="cryptoComApiKey" value="${env_var:CRYPTO_COM_API_KEY}"/>
        <mapEntry key="cryptoComApiSecret" value="${env_var:CRYPTO_COM_API_SECRET}"/>
        <mapEntry key="emailAccount" value="${env_var:BURNINGWAVE_ORG_ACCOUNT_NAME}"/>
        <mapEntry key="emailPassword" value="${env_var:BURNINGWAVE_ORG_ACCOUNT_PASSWORD}"/>
        <mapEntry key="multiThreadingMode" value="normal"/>
    </mapAttribute>
    <booleanAttribute key="org.eclipse.jdt.launching.ATTR_ATTR_USE_ARGFILE" value="false"/>
    <booleanAttribute key="org.eclipse.jdt.launching.ATTR_EXCLUDE_TEST_CODE" value="true"/>
    <booleanAttribute key="org.eclipse.jdt.launching.ATTR_SHOW_CODEDETAILS_IN_EXCEPTION_MESSAGES" value="true"/>
    <booleanAttribute key="org.eclipse.jdt.launching.ATTR_USE_CLASSPATH_ONLY_JAR" value="false"/>
    <stringAttribute key="org.eclipse.jdt.launching.CLASSPATH_PROVIDER" value="org.eclipse.m2e.launchconfig.classpathProvider"/>
    <stringAttribute key="org.eclipse.jdt.launching.JRE_CONTAINER" value="org.eclipse.jdt.launching.JRE_CONTAINER/org.eclipse.jdt.internal.debug.ui.launcher.StandardVMType/21/"/>
    <stringAttribute key="org.eclipse.jdt.launching.MAIN_TYPE" value="org.rg.service.Application"/>
    <stringAttribute key="org.eclipse.jdt.launching.MODULE_NAME" value="rg-services"/>
    <stringAttribute key="org.eclipse.jdt.launching.PROGRAM_ARGUMENTS" value="--spring.config.location=file:///${project_loc}\my-favourite-assets-scanner.yml"/>
    <stringAttribute key="org.eclipse.jdt.launching.PROJECT_ATTR" value="trading-utils"/>
    <stringAttribute key="org.eclipse.jdt.launching.SOURCE_PATH_PROVIDER" value="org.eclipse.m2e.launchconfig.sourcepathProvider"/>
</launchConfiguration>
